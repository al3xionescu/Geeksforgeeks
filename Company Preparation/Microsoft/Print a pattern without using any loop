#include <iostream>

using namespace std;
void printPattern(int n, int m, bool ok){
    cout<<m<<' ';
    if (ok == false && n==m)
        return;
    if(ok){
        if(m-5>0)
            printPattern(n, m-5,true);
        else
            printPattern(n,m-5,false);
    }
    else{
        printPattern(n,m+5,false);
    }
}
int main()
{
    int n = 16;
    printPattern(n,n,true);

    return 0;
}
